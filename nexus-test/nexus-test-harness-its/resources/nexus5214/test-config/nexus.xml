      <browseable>true</browseable>
      <writePolicy>READ_ONLY</writePolicy>
      <indexable>true</indexable>
      <searchable>true</searchable>
      <localStorage>
        <provider>file</provider>
      </localStorage>
      <remoteStorage>
        <url>http://repository.sonatype.org/content/groups/forge/</url>
        <httpProxySettings>
          <proxyHostname>localhost</proxyHostname>
          <proxyPort>${webproxy-server-port}</proxyPort>
        </httpProxySettings>
      </remoteStorage>
      <externalConfiguration>
        <repositoryPolicy>RELEASE</repositoryPolicy>
        <checksumPolicy>WARN</checksumPolicy>
        <fileTypeValidation>true</fileTypeValidation>
        <downloadRemoteIndex>true</downloadRemoteIndex>
        <artifactMaxAge>0</artifactMaxAge>
        <metadataMaxAge>0</metadataMaxAge>
