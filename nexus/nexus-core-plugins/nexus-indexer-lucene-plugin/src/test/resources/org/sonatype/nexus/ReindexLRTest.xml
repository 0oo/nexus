<plexus>
  <components>
    <component>
      <role>org.sonatype.jettytestsuite.ServletServer</role>
      <implementation>
        org.sonatype.jettytestsuite.ServletServer</implementation>
      <configuration>
        <port>${proxy.server.port}</port>
        <webappContexts>
          <webappContext>
            <name>central</name>
            <servletInfos>
              <servletInfo>
                <mapping>/*</mapping>
                <servletClass>
                  org.mortbay.jetty.servlet.DefaultServlet</servletClass>
                <parameters>
                  <property>
                    <name>resourceBase</name>
                    <value>${basedir}/target/test-classes/reposes-remote/central</value>
                  </property>
                  <property>
                    <name>dirAllowed</name>
                    <value>true</value>
                  </property>
                </parameters>
              </servletInfo>
            </servletInfos>
          </webappContext>
          <webappContext>
            <name>central-inc1</name>
            <servletInfos>
              <servletInfo>
                <mapping>/*</mapping>
                <servletClass>org.mortbay.jetty.servlet.DefaultServlet</servletClass>
                <parameters>
                  <property>
                    <name>resourceBase</name>
                    <value>${basedir}/target/test-classes/reposes-remote/central-inc1</value>
                  </property>
                  <property>
                    <name>dirAllowed</name>
                    <value>true</value>
                  </property>
                </parameters>
              </servletInfo>
            </servletInfos>
          </webappContext>
          <webappContext>
            <name>central-inc2</name>
            <servletInfos>
              <servletInfo>
                <mapping>/*</mapping>
                <servletClass>org.mortbay.jetty.servlet.DefaultServlet</servletClass>
                <parameters>
                  <property>
                    <name>resourceBase</name>
                    <value>${basedir}/target/test-classes/reposes-remote/central-inc2</value>
                  </property>
                  <property>
                    <name>dirAllowed</name>
                    <value>true</value>
                  </property>
                </parameters>
              </servletInfo>
            </servletInfos>
          </webappContext>
          <webappContext>
            <name>central-inc3</name>
            <servletInfos>
              <servletInfo>
                <mapping>/*</mapping>
                <servletClass>org.mortbay.jetty.servlet.DefaultServlet</servletClass>
                <parameters>
                  <property>
                    <name>resourceBase</name>
                    <value>${basedir}/target/test-classes/reposes-remote/central-inc3</value>
                  </property>
                  <property>
                    <name>dirAllowed</name>
                    <value>true</value>
                  </property>
                </parameters>
              </servletInfo>
            </servletInfos>
          </webappContext>
          <webappContext>
            <name>central-inc1-v1</name>
            <servletInfos>
              <servletInfo>
                <mapping>/*</mapping>
                <servletClass>org.mortbay.jetty.servlet.DefaultServlet</servletClass>
                <parameters>
                  <property>
                    <name>resourceBase</name>
                    <value>
                      ${basedir}/target/test-classes/reposes-remote/central-inc1-v1
                    </value>
                  </property>
                  <property>
                    <name>dirAllowed</name>
                    <value>true</value>
                  </property>
                </parameters>
              </servletInfo>
            </servletInfos>
          </webappContext>
          <webappContext>
            <name>central-inc2-v1</name>
            <servletInfos>
              <servletInfo>
                <mapping>/*</mapping>
                <servletClass>
                  org.mortbay.jetty.servlet.DefaultServlet
                </servletClass>
                <parameters>
                  <property>
                    <name>resourceBase</name>
                    <value>
                      ${basedir}/target/test-classes/reposes-remote/central-inc2-v1
                    </value>
                  </property>
                  <property>
                    <name>dirAllowed</name>
                    <value>true</value>
                  </property>
                </parameters>
              </servletInfo>
            </servletInfos>
          </webappContext>
          <webappContext>
            <name>central-inc3-v1</name>
            <servletInfos>
              <servletInfo>
                <mapping>/*</mapping>
                <servletClass>
                  org.mortbay.jetty.servlet.DefaultServlet
                </servletClass>
                <parameters>
                  <property>
                    <name>resourceBase</name>
                    <value>
                      ${basedir}/target/test-classes/reposes-remote/central-inc3-v1
                    </value>
                  </property>
                  <property>
                    <name>dirAllowed</name>
                    <value>true</value>
                  </property>
                </parameters>
              </servletInfo>
            </servletInfos>
          </webappContext>
        </webappContexts>
      </configuration>
    </component>
  </components>
</plexus>
